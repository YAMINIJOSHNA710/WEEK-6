function DateTime() {
  const now = new Date();
  const currentDate = now.toLocaleDateString();
  const currentTime = now.toLocaleTimeString();

  return (
    <div>
      <p>Current Date: {currentDate}</p>
      <p>Current Time: {currentTime}</p>
    </div>
  );
}

export default DateTime;


import DateTime from './DateTime';

function App() {
  return (
    <div>
      <Header />
      <DateTime />
      <Footer />
    </div>
  );
}

export default App;
